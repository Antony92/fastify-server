{
	"name": "fastify-server",
	"version": "1.0.0",
	"description": "",
	"main": "dist/app.js",
	"type": "module",
	"scripts": {
		"dev": "node --watch-path ./src --import tsx/esm ./src/app.ts",
		"build": "tsc -p .",
		"lint": "npx eslint .",
		"tests": "node --import tsx/esm --test",
		"test:main": "node --import tsx/esm tests/main.ts",
		"prisma:generate": "npx prisma generate",
		"prisma:studio": "npx prisma studio",
		"prisma:push": "npx prisma db push",
		"prisma:pull": "npx prisma db pull",
		"prisma:dev": "npx prisma migrate dev",
		"prisma:deploy": "npx prisma migrate deploy",
		"prisma:reset": "npx prisma migrate reset",
		"encode-env": "base64 < .env | pbcopy",
		"decode-env": "pbpaste | base64 -d"
	},
	"author": "aasenov",
	"license": "MIT",
	"dependencies": {
		"@fastify/compress": "~8.3.1",
		"@fastify/cookie": "~11.0.2",
		"@fastify/cors": "~11.2.0",
		"@fastify/jwt": "~10.0.0",
		"@fastify/multipart": "~9.4.0",
		"@fastify/oauth2": "~8.2.0",
		"@fastify/rate-limit": "~10.3.0",
		"@fastify/schedule": "~6.0.0",
		"@fastify/sse": "~0.4.0",
		"@fastify/static": "~9.0.0",
		"@fastify/swagger": "~9.7.0",
		"@fastify/swagger-ui": "~5.2.5",
		"@fastify/websocket": "~11.2.0",
		"@prisma/adapter-pg": "~7.4.0",
		"@prisma/client": "~7.4.0",
		"croner": "~10.0.1",
		"date-fns": "~4.1.0",
		"dotenv": "~17.3.1",
		"fastify": "~5.7.4",
		"handlebars": "~4.7.8",
		"nodemailer": "~8.0.1",
		"pg": "~8.18.0",
		"sanitize-html": "~2.17.0",
		"toad-scheduler": "~3.1.0",
		"undici": "~7.22.0"
	},
	"devDependencies": {
		"@eslint/js": "~10.0.1",
		"@types/node": "~25.2.3",
		"@types/nodemailer": "~7.0.10",
		"@types/pg": "~8.16.0",
		"@types/sanitize-html": "~2.16.0",
		"@types/ws": "~8.18.1",
		"eslint": "~10.0.0",
		"json-schema-to-ts": "~3.1.1",
		"prisma": "~7.4.0",
		"tsx": "~4.21.0",
		"typescript": "~5.9.3",
		"typescript-eslint": "~8.55.0"
	}
}
