{
	"name": "fastify-server",
	"version": "1.0.0",
	"description": "",
	"main": "dist/app.js",
	"type": "module",
	"scripts": {
		"dev": "node --watch-path ./src --import tsx/esm ./src/app.ts",
		"build": "tsc -p .",
		"lint": "npx eslint .",
		"test:ui": "vitest --ui",
		"test:run": "vitest run",
		"test:main": "node --import tsx/esm src/tests/main.ts",
		"prisma:generate": "npx prisma generate",
		"prisma:studio": "npx prisma studio",
		"prisma:push": "npx prisma db push",
		"prisma:pull": "npx prisma db pull",
		"prisma:dev": "npx prisma migrate dev",
		"prisma:deploy": "npx prisma migrate deploy",
		"prisma:reset": "npx prisma migrate reset",
		"encode-env": "base64 < .env | pbcopy",
		"decode-env": "pbpaste | base64 -d"
	},
	"author": "aasenov",
	"license": "MIT",
	"dependencies": {
		"@azure/msal-node": "~3.5.3",
		"@fastify/compress": "~8.0.1",
		"@fastify/cookie": "~11.0.2",
		"@fastify/cors": "~11.0.1",
		"@fastify/jwt": "~9.1.0",
		"@fastify/multipart": "~9.0.3",
		"@fastify/oauth2": "~8.1.2",
		"@fastify/rate-limit": "~10.2.2",
		"@fastify/static": "~8.1.1",
		"@fastify/swagger": "~9.5.1",
		"@fastify/swagger-ui": "~5.2.2",
		"@fastify/websocket": "~11.0.2",
		"@fastify/schedule": "~6.0.0",
		"@prisma/client": "~6.7.0",
		"ajv": "~8.17.1",
		"ajv-keywords": "~5.1.0",
		"dotenv": "~16.5.0",
		"fastify": "~5.3.2",
		"handlebars": "~4.7.8",
		"nodemailer": "~6.10.1",
		"sanitize-html": "~2.16.0",
		"date-fns": "~4.1.0",
		"toad-scheduler": "~3.1.0",
		"croner": "~9.0.0",
		"undici": "~7.9.0"
	},
	"devDependencies": {
		"@eslint/js": "~9.26.0",
		"@types/node": "~22.15.17",
		"@types/nodemailer": "~6.4.17",
		"@types/sanitize-html": "~2.16.0",
		"@types/ws": "~8.18.1",
		"@vitest/ui": "~3.1.3",
		"eslint": "~9.26.0",
		"prisma": "~6.7.0",
		"tsx": "~4.19.4",
		"typescript": "~5.8.3",
		"typescript-eslint": "~8.32.1",
		"vitest": "~3.1.3"
	}
}
